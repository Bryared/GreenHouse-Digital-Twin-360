#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/bin/node_modules:/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/node_modules:/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/jstransform@11.0.3/node_modules:/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/bin/node_modules:/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/jstransform@11.0.3/node_modules/jstransform/node_modules:/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/jstransform@11.0.3/node_modules:/mnt/c/Proyectos/GreenHouse-Digital-Twin-360/3_frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jstransform/bin/jstransform" "$@"
else
  exec node  "$basedir/../jstransform/bin/jstransform" "$@"
fi
